        <t t-call="web.html_container">
            <t t-if="not o" t-set="o" t-value="doc"/>
            <div class="article" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-out="0"/>
            </div>
            <div t-attf-class="footer agreement-footer">
                <div class="row mt-2">
                    <div class="web-info">
                        <p>©SCAR, LLC dba GlassTech | 1445 Jupiter Park Dr. STE #11, Jupiter, FL 33458 |  
                            <b> www.gtrestore.com </b>
                        </p>
                    </div>
                    <div class="pager">
                        <p class="page-no">Pg <span class="page"/> of <span class="topage"/></p>
                        <p>Service Agreement 2020</p>
                    </div>
                </div>
            </div>
        </t>
